programa{

	funcao inicio(){

		escreva("\n38) Escreva um programa que mostre na tela a seguinte contagem: 6 7 8 9 10 11 Acabou!\n")
		inteiro contador 
		contador = 6

		escreva("\nIniciando contagem progressiva a partir do n√∫mero 6 ...\n")
