programa{
	
	funcao inicio(){
		
		escreva("61) Crie um programa que mostre na tela a seguinte contagem, usando a estrutura “faça enquanto” 0 3 6 9 12 15 18 21 24 27 30 Acabou!\n\n")
		inteiro numero = 0 

		faca {

			escreva(numero+" ")
			numero += 3
			
		} enquanto (numero < 31)
		escreva("Acabou!\n")
	}
}
