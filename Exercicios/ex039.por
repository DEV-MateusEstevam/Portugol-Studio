programa{
	
	funcao inicio(){

		escreva("39) Faça um algoritmo que mostre na tela a seguinte contagem: 10 9 8 7 6 5 4 3 Acabou!\n")
				
		inteiro contador
		contador = 10
		
		escreva("\nIniciando a contagem regressiva a partir do número 10 ...\n")

		enquanto (contador > 2){
			
		escreva(""+contador+", ")
			contador--
		}

		escreva ("Acabou!\n")
	}
}
